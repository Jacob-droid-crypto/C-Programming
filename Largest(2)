// 
#include <stdio.h>

int main() 
{
    int num1,num2;
    printf("Enter the numbers: ");
    scanf("%d %d",&num1,&num2);
    if(num1>num2)
    {
        printf("Number %d is greater than %d",num1,num2);
    }
    else if(num2>num1)
    {
        printf("Number %d is greater than %d",num2,num1);
    }
    else
    {
        printf("Invalid input");
    }
    return 0;
}
